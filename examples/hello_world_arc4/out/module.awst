abstract contract ARC4Contract
{
  clear_state_program(): bool
  {
    return true
  }
}

contract HelloWorldContract extends (algopy.arc4.ARC4Contract)
{
  approval_program(): bool
  {
    return arc4_router()
  }
  
  baremethod __algopy_default_create(): void
  {
  }
  
  abimethod hello(name: string): string
  {
    return 'Hello, ' + name
  }
}