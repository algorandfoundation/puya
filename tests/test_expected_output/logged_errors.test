## case: test_logged_assert_invalid_code

from algopy import Contract, Txn, logged_assert


class MyContract(Contract):
    def approval_program(self) -> bool:
        logged_assert(Txn.num_app_args > 0, "not-alnum!") ## W: error code should be alphanumeric
        return True

    def clear_state_program(self) -> bool:
        return True

