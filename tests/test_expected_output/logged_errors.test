    logged_err("01") ## E: logged errors are not supported in logic signatures
    return True

## case: test_logged_assert_in_logicsig

from algopy import logicsig, logged_assert, op


@logicsig
def my_sig() -> bool:
    logged_assert(op.arg(0) != b"", "01") ## E: logged errors are not supported in logic signatures
    return True

