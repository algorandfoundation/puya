abstract contract ARC4Contract
{
  clear_state_program(): bool
  {
    return true
  }
}

contract MyContract
{
  approval_program(): bool
  {
    a: bytes = txna<ApplicationArgs, 0>()
    b: bytes = txna<ApplicationArgs, 1>()
    assert(len(a + b) != 0u)
    return len(b + a) > 0u
  }
  
  clear_state_program(): bool
  {
    return true
  }
}