#pragma version 10
#pragma typetrack false

// test_cases.literals.folding.LiteralFolding.approval_program() -> uint64:
main:
    intcblock 0 1
    bytecblock "a" "b"
    bytec_0 // "a"
    bytec_1 // "b"
    callsub is_substring
    !
    assert
    bytec_0 // "a"
    bytec_1 // "b"
    callsub is_substring
    !
    assert
    bytec_0 // "a"
    pushbytes "abc"
    callsub is_substring
    return


// _puya_lib.bytes_.is_substring(item: bytes, sequence: bytes) -> uint64:
is_substring:
    proto 2 1
    pushbytes ""
    intc_0 // 0

is_substring_while_top@1:
    frame_dig -2
    len
    frame_dig 1
    +
    dup
    frame_bury 0
    frame_dig -1
    len
    <=
    bz is_substring_after_while@5
    frame_dig -1
    frame_dig 1
    frame_dig 0
    substring3
    frame_dig -2
    ==
    bz is_substring_after_if_else@4
    intc_1 // 1
    frame_bury 0
    retsub

is_substring_after_if_else@4:
    frame_dig 1
    intc_1 // 1
    +
    frame_bury 1
    b is_substring_while_top@1

is_substring_after_while@5:
    intc_0 // 0
    frame_bury 0
    retsub
