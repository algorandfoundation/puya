#pragma version 11
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    txn ApplicationID
    bnz main_after_if_else@2
    pushbytes "gvalue"
    pushint 4 // 4
    app_global_put

main_after_if_else@2:
    txn NumAppArgs
    !
    assert
    txn OnCompletion
    !
    txn ApplicationID
    !
    &&
    assert // OnCompletion must be NoOp && can only call when creating
    pushint 1 // 1
    return
