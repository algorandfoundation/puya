contract MyContract
{
  approval_program(): bool
  {
    assert(test_cases.string_ops.contract::is_in_str(hex<"62726F776E20666F78">, hex<"54686520717569636B2062726F776E20666F78206A756D706564206F76657220746865206C617A7920646F67">))
    assert(!(test_cases.string_ops.contract::is_in_str(hex<"72656420666F78">, hex<"54686520717569636B2062726F776E20666F78206A756D706564206F76657220746865206C617A7920646F67">)))
    return true
  }
  
  clear_state_program(): bool
  {
    return true
  }
}

subroutine is_in_str(a: bytes, b: bytes): bool
{
  return algopy_lib_bytes::is_substring(a, b)
}