#pragma version 10
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    intcblock 0 1 1000
    txn ApplicationID
    !
    txn OnCompletion
    intc_1 // 1
    shl
    +
    switch call_NoOp create_NoOp *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED *NOT_IMPLEMENTED

*NOT_IMPLEMENTED:
    err // The requested action is not implemented in this contract. Are you using the correct OnComplete? Did you set your app ID?

create_NoOp:
    txn NumAppArgs
    !
    return // on error: this contract does not implement the given ABI method for create NoOp

call_NoOp:
    pushbytes 0x48e9ed7c // method "create_and_transfer()void"
    txna ApplicationArgs 0
    match create_and_transfer_route@3
    err // this contract does not implement the given ABI method for call NoOp

create_and_transfer_route@3:
    itxn_begin
    global CurrentApplicationAddress
    dup
    itxn_field ConfigAssetClawback
    itxn_field ConfigAssetManager
    intc_0 // 0
    itxn_field ConfigAssetDecimals
    pushbytes "TST"
    itxn_field ConfigAssetUnitName
    pushbytes "test"
    itxn_field ConfigAssetName
    intc_2 // 1000
    itxn_field ConfigAssetTotal
    pushint 3 // acfg
    itxn_field TypeEnum
    intc_0 // 0
    itxn_field Fee
    itxn_submit
    itxn CreatedAssetID
    itxn_begin
    dup
    asset_params_get AssetCreator
    assert // asset exists
    global CurrentApplicationAddress
    uncover 2
    itxn_field XferAsset
    intc_2 // 1000
    itxn_field AssetAmount
    itxn_field AssetReceiver
    itxn_field AssetSender
    pushint 4 // axfer
    itxn_field TypeEnum
    intc_0 // 0
    itxn_field Fee
    itxn_submit
    intc_1 // 1
    return
