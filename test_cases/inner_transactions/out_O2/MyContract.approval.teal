// test_cases.inner_transactions.contract.MyContract.test2() -> void:
test2:
    proto 0 0
    int 0
    dupn 3
    byte ""
    dupn 2
    txn NumAppArgs
    bz test2_else_body@2
    byte 0x31
    frame_bury 0
    byte 0x32
    frame_bury 1
    int 2
    frame_bury 4
    int NoOp
    frame_bury 6
    int 1
    frame_bury 5
    byte "with args param set"
    b test2_after_if_else@3

test2_else_body@2:
    byte 0x33
    frame_bury 0
    byte 0x34
    frame_bury 1
    byte 0x35
    frame_bury 2
    int 3
    frame_bury 4
    byte "no args param set"
    int 0
    frame_bury 5

test2_after_if_else@3:
    itxn_begin
    itxn_field Note
    frame_dig 5
    int 1
    >=
    bz test2_next_field@5
    frame_dig 6
    itxn_field OnCompletion

test2_next_field@5:
    frame_dig 0
    itxn_field ApplicationArgs
    frame_dig 1
    itxn_field ApplicationArgs
    frame_dig 4
    int 3
    >=
    bz test2_next_field@7
    frame_dig 2
    itxn_field ApplicationArgs

test2_next_field@7:
    byte 0x098101
    itxn_field ClearStateProgramPages
    byte 0x098101
    itxn_field ApprovalProgramPages
    int appl
    itxn_field TypeEnum
    int 0
    itxn_field Fee
    itxn_submit
    itxn Note
    frame_bury 3
    itxna ApplicationArgs 0
    byte "1"
    ==
    assert // correct args used 1
    itxna ApplicationArgs 1
    byte "2"
    ==
    assert // correct args used 2
    txn NumAppArgs
    int 1
    >
    bz test2_after_if_else@11
    itxn_begin
    byte 0x3432
    itxn_field ApplicationArgs
    int DeleteApplication
    itxn_field OnCompletion
    byte 0x098101
    itxn_field ClearStateProgramPages
    byte 0x098101
    itxn_field ApprovalProgramPages
    int appl
    itxn_field TypeEnum
    int 0
    itxn_field Fee
    itxn_submit
    itxna ApplicationArgs 0
    byte "42"
    ==
    assert // correct args used 2
    frame_dig 3
    byte "with args param set"
    ==
    assert

test2_after_if_else@11:
    retsub


