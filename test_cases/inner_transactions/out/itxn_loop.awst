contract MyContract
{
  approval_program(): bool
  {
    note: puyapy.Bytes = 'ABCDE'
    app_params: puyapy.itxn.ApplicationCallTransactionParams = create_inner_transaction(TypeEnum=appl, ApprovalProgramPages=hex<"0A8101">, ClearStateProgramPages=hex<"0A8101">, OnCompletion=DeleteApplication, Note='', Fee=0u)
    for i in range(0u, 4u, 1u) {
      i_note: puyapy.Bytes = extract3(note, 0u, i)
      switch (i) {
        case 1u: {
          update_inner_transaction(app_params,Note=i_note, ApplicationArgs=('1'))
        }
        case 2u: {
          update_inner_transaction(app_params,Note=i_note, ApplicationArgs=('2', '1'))
        }
        case 3u: {
          update_inner_transaction(app_params,Note=i_note, ApplicationArgs=('3', '2', '1'))
        }
      }
      app_txn: puyapy.itxn.ApplicationCallInnerTransaction = submit_txn(app_params)
      log(app_txn.Note)
      log(itob(app_txn.NumAppArgs))
    }
    return true
  }
  
  clear_state_program(): bool
  {
    return true
  }
}