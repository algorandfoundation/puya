__init__:
    proto 0 0
    bytec_1 // "counter"
    intc_0 // 0
    app_global_put
    bytec_3 // "global_uint"
    intc_0 // 0
    app_global_put
    bytec 4 // "global_bytes"
    pushbytes 0x
    app_global_put
    retsub


