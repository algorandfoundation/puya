#pragma version 10
#pragma typetrack false

// test_cases.compile.apps.HelloBase.__algopy_entrypoint_with_init() -> uint64:
main:
    bytecblock "greeting"
    txn ApplicationID
    bnz after_if_else@2
    bytec_0 // "greeting"
    pushbytes ""
    app_global_put

after_if_else@2:
    txn NumAppArgs
    bz bare_routing@7
    pushbytess 0x24378d3c 0xd0a28200 // method "delete()void", method "greet(string)string"
    txna ApplicationArgs 0
    match delete_route@5 greet_route@6
    err

greet_route@6:
    txn OnCompletion
    !
    txn ApplicationID
    &&
    assert // OnCompletion must be NoOp && can only call when not creating
    txna ApplicationArgs 1
    extract 2 0
    pushint 0 // 0
    bytec_0 // "greeting"
    app_global_get_ex
    assert // check self.greeting exists
    pushbytes " "
    concat
    swap
    concat
    dup
    len
    itob
    extract 6 2
    swap
    concat
    pushbytes 0x151f7c75
    swap
    concat
    log
    pushint 1 // 1
    return

delete_route@5:
    txn OnCompletion
    pushint 5 // DeleteApplication
    ==
    txn ApplicationID
    &&
    return // on error: OnCompletion must be DeleteApplication && can only call when not creating

bare_routing@7:
    pushints 0 4 // NoOp, UpdateApplication
    txn OnCompletion
    match __algopy_default_create@8 update@9
    err

update@9:
    txn ApplicationID
    return // on error: can only call when not creating

__algopy_default_create@8:
    txn ApplicationID
    !
    return // on error: can only call when creating
