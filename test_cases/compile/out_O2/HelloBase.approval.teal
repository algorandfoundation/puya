#pragma version 10
#pragma typetrack false

// test_cases.compile.apps.HelloBase.__algopy_entrypoint_with_init() -> uint64:
main:
    bytecblock "greeting"
    pushint 1 // 1
    txn ApplicationID
    bnz main_after_if_else@2
    bytec_0 // "greeting"
    pushbytes ""
    app_global_put

main_after_if_else@2:
    txn NumAppArgs
    bz main_bare_routing@8
    pushbytess 0x24378d3c 0xd0a28200 // method "delete()void", method "greet(string)string"
    txna ApplicationArgs 0
    match main_delete_route@5 main_greet_route@6
    err

main_greet_route@6:
    txn OnCompletion
    !
    assert // OnCompletion is not NoOp
    txn ApplicationID
    assert // can only call when not creating
    txna ApplicationArgs 1
    extract 2 0
    pushint 0 // 0
    bytec_0 // "greeting"
    app_global_get_ex
    assert // check self.greeting exists
    pushbytes " "
    concat
    swap
    concat
    dup
    len
    itob
    extract 6 2
    swap
    concat
    pushbytes 0x151f7c75
    swap
    concat
    log
    return

main_delete_route@5:
    txn OnCompletion
    pushint 5 // DeleteApplication
    ==
    assert // OnCompletion is not DeleteApplication
    txn ApplicationID
    return // on error: can only call when not creating

main_bare_routing@8:
    pushints 4 0 // 4, 0
    txn OnCompletion
    match main_update@9 main___algopy_default_create@10
    err

main___algopy_default_create@10:
    txn ApplicationID
    !
    return // on error: can only call when creating

main_update@9:
    txn ApplicationID
    return // on error: can only call when not creating
