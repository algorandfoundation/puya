#pragma version 10
#pragma typetrack false

// test_cases.compile.apps.Hello.__algopy_entrypoint_with_init() -> uint64:
main:
    bytecblock "greeting"
    txn ApplicationID
    bnz after_if_else@2
    bytec_0 // "greeting"
    pushbytes ""
    app_global_put

after_if_else@2:
    txn NumAppArgs
    bz update@12
    pushbytes 0x24378d3c // method "delete()void"
    txna ApplicationArgs 0
    match delete_route@5
    txn OnCompletion
    !
    assert // OnCompletion must be NoOp
    txn ApplicationID
    bz create_NoOp@9
    pushbytes 0xd0a28200 // method "greet(string)string"
    txna ApplicationArgs 0
    match greet_route@8
    err

greet_route@8:
    txna ApplicationArgs 1
    extract 2 0
    pushint 0 // 0
    bytec_0 // "greeting"
    app_global_get_ex
    assert // check self.greeting exists
    pushbytes " "
    concat
    swap
    concat
    dup
    len
    itob
    extract 6 2
    swap
    concat
    pushbytes 0x151f7c75
    swap
    concat
    log
    pushint 1 // 1
    return

create_NoOp@9:
    pushbytes 0x20df3a54 // method "create(string)void"
    txna ApplicationArgs 0
    match create_route@10
    err

create_route@10:
    txna ApplicationArgs 1
    extract 2 0
    bytec_0 // "greeting"
    swap
    app_global_put
    pushint 1 // 1
    return

delete_route@5:
    txn OnCompletion
    pushint 5 // DeleteApplication
    ==
    txn ApplicationID
    &&
    return // on error: OnCompletion must be DeleteApplication && can only call when not creating

update@12:
    txn OnCompletion
    pushint 4 // UpdateApplication
    ==
    txn ApplicationID
    &&
    return // on error: OnCompletion must be UpdateApplication && can only call when not creating
