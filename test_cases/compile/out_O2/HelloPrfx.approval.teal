#pragma version 11
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    intcblock 1 0
    bytecblock "greeting" PRFX_GREETING
    txn ApplicationID
    bnz main_after_if_else@2
    bytec_0 // "greeting"
    bytec_1 // PRFX_GREETING
    app_global_put

main_after_if_else@2:
    txn NumAppArgs
    bz main_update@14
    pushbytes 0x24378d3c // method "delete()void"
    txna ApplicationArgs 0
    match main_delete_route@5
    txn OnCompletion
    !
    assert // OnCompletion must be NoOp
    txn ApplicationID
    bz main_create_NoOp@10
    pushbytes 0xd0a28200 // method "greet(string)string"
    txna ApplicationArgs 0
    match main_greet_route@8
    err

main_greet_route@8:
    txna ApplicationArgs 1
    dup
    intc_1 // 0
    extract_uint16
    pushint 2 // 2
    +
    dig 1
    len
    ==
    assert // invalid number of bytes for (len+utf8[])
    extract 2 0
    intc_1 // 0
    bytec_0 // "greeting"
    app_global_get_ex
    assert // check self.greeting exists
    pushbytes " "
    concat
    swap
    concat
    dup
    len
    itob
    extract 6 2
    swap
    concat
    pushbytes 0x151f7c75
    swap
    concat
    log
    intc_0 // 1
    return

main_create_NoOp@10:
    pushbytes 0x4c5c61ba // method "create()void"
    txna ApplicationArgs 0
    match main_create_route@11
    err

main_create_route@11:
    intc_0 // 1
    return

main_delete_route@5:
    txn OnCompletion
    pushint 5 // DeleteApplication
    ==
    txn ApplicationID
    &&
    assert // OnCompletion must be DeleteApplication && can only call when not creating
    intc_0 // 1
    return

main_update@14:
    txn OnCompletion
    pushint 4 // UpdateApplication
    ==
    txn ApplicationID
    &&
    assert // OnCompletion must be UpdateApplication && can only call when not creating
    intc_0 // 1
    return
