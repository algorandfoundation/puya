#pragma version 10
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    pushint 1 // 1
    txn NumAppArgs
    bz main_bare_routing@7
    pushbytess 0x24378d3c 0xf5e4fd4d // method "delete()void", method "get_big_bytes_length()uint64"
    txna ApplicationArgs 0
    match main_delete_route@3 main_get_big_bytes_length_route@4
    err

main_get_big_bytes_length_route@4:
    txn OnCompletion
    !
    assert // OnCompletion is not NoOp
    txn ApplicationID
    assert // can only call when not creating
    pushbytes 0x151f7c750000000000001000
    log
    return

main_delete_route@3:
    txn OnCompletion
    pushint 5 // DeleteApplication
    ==
    assert // OnCompletion is not DeleteApplication
    txn ApplicationID
    assert // can only call when not creating
    return

main_bare_routing@7:
    txn OnCompletion
    !
    assert
    txn ApplicationID
    !
    assert // can only call when creating
    return
