#pragma version 10

test_cases.compile.apps.LargeProgram.approval_program:
    txn NumAppArgs
    bz main_bare_routing@6
    method "get_big_bytes_length()uint64"
    method "delete()void"
    txna ApplicationArgs 0
    match main_get_big_bytes_length_route@2 main_delete_route@3
    err // reject transaction

main_get_big_bytes_length_route@2:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub get_big_bytes_length
    itob
    byte 0x151f7c75
    swap
    concat
    log
    int 1
    return

main_delete_route@3:
    txn OnCompletion
    int DeleteApplication
    ==
    assert // OnCompletion is DeleteApplication
    txn ApplicationID
    assert // is not creating
    callsub delete
    int 1
    return

main_bare_routing@6:
    txn OnCompletion
    !
    assert // reject transaction
    txn ApplicationID
    !
    assert // is creating
    int 1
    return


// test_cases.compile.apps.LargeProgram.get_big_bytes_length() -> uint64:
get_big_bytes_length:
    proto 0 1
    callsub get_big_bytes
    len
    retsub


// test_cases.compile.apps.get_big_bytes() -> bytes:
get_big_bytes:
    proto 0 1
    byte 
    retsub


// test_cases.compile.apps.LargeProgram.delete() -> void:
delete:
    proto 0 0
    retsub
