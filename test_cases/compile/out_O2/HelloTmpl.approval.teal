#pragma version 10
#pragma typetrack false

// test_cases.compile.apps.HelloTmpl.__algopy_entrypoint_with_init() -> uint64:
main:
    bytecblock "greeting" TMPL_GREETING
    pushint 1 // 1
    txn ApplicationID
    bnz main_after_if_else@2
    bytec_0 // "greeting"
    bytec_1 // TMPL_GREETING
    app_global_put

main_after_if_else@2:
    txn NumAppArgs
    bz main_bare_routing@9
    pushbytes 0x24378d3c // method "delete()void"
    txna ApplicationArgs 0
    match main_delete_route@5
    txn OnCompletion
    !
    assert // OnCompletion is not NoOp
    txn ApplicationID
    bz main_create_NoOp@8
    txna ApplicationArgs 0
    pushbytes 0xd0a28200 // method "greet(string)string"
    ==
    assert
    txna ApplicationArgs 1
    extract 2 0
    pushint 0 // 0
    bytec_0 // "greeting"
    app_global_get_ex
    assert // check self.greeting exists
    pushbytes " "
    concat
    swap
    concat
    dup
    len
    itob
    extract 6 2
    swap
    concat
    pushbytes 0x151f7c75
    swap
    concat
    log
    return

main_create_NoOp@8:
    txna ApplicationArgs 0
    pushbytes 0x4c5c61ba // method "create()void"
    ==
    return

main_delete_route@5:
    txn OnCompletion
    pushint 5 // DeleteApplication
    ==
    assert // OnCompletion is not DeleteApplication
    txn ApplicationID
    return // on error: can only call when not creating

main_bare_routing@9:
    txn OnCompletion
    pushint 4 // 4
    ==
    assert
    txn ApplicationID
    return // on error: can only call when not creating
