#pragma version 10
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    intcblock 2 1
    pushbytes ""
    txn NumAppArgs
    bz main_bare_routing@6
    pushbytes 0xd3ad7a55 // method "method(uint64)void"
    txna ApplicationArgs 0
    match main_method_route@3

main_after_if_else@10:
    err

main_method_route@3:
    txn OnCompletion
    !
    assert // OnCompletion is not NoOp
    txn ApplicationID
    assert // can only call when not creating
    txna ApplicationArgs 1
    btoi
    bury 1

main_while_top@14:
    global OpcodeBudget
    pushint 350 // 350
    >
    bz main_if_body@17
    txn Sender
    sha3_256
    txn Sender
    !=
    assert
    b main_while_top@14

main_if_body@17:
    dup
    intc_0 // 2
    +
    intc_0 // 2
    -
    dig 1
    ==
    assert
    intc_1 // 1
    return

main_bare_routing@6:
    txn OnCompletion
    bnz main_after_if_else@10
    txn ApplicationID
    !
    assert // can only call when creating
    intc_1 // 1
    return
