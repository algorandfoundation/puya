#pragma version 10
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    pushbytes ""
    txn NumAppArgs
    bz __algopy_default_create@6
    pushbytes 0xd3ad7a55 // method "method(uint64)void"
    txna ApplicationArgs 0
    match method_route@3
    err

method_route@3:
    txn OnCompletion
    !
    txn ApplicationID
    &&
    assert // OnCompletion must be NoOp && can only call when not creating
    txna ApplicationArgs 1
    btoi
    bury 1

while_top@10:
    global OpcodeBudget
    pushint 350 // 350
    >
    bz if_body@13
    txn Sender
    sha3_256
    txn Sender
    !=
    assert
    b while_top@10

if_body@13:
    dup
    pushint 2 // 2
    +
    pushint 2 // 2
    -
    dig 1
    ==
    return

__algopy_default_create@6:
    txn OnCompletion
    !
    txn ApplicationID
    !
    &&
    return // on error: OnCompletion must be NoOp && can only call when creating
