main:
    txn NumAppArgs
    bz __algopy_default_create@10
    txn OnCompletion
    !
    assert // OnCompletion must be NoOp
    txn ApplicationID
    assert
    pushbytess 0x1e3e4374 0xd7a938c9 // method "oh_no(bool)void", method "oh_yes(bool)void"
    txna ApplicationArgs 0
    match oh_no_route@4 oh_no_route@4
