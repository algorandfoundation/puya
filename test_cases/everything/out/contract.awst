ZERO = 0
ZER0 = 0
ONE = 1

subroutine get_banned(): puyapy.Account
{
  addr: puyapy.Account = Address("VCMJKWOY5P5P7SKMZFFOCEROPJCZOTIJMNIYNUCKH7LRO45JMJP6UYBIJA")
  return addr
}

subroutine add_one(x: puyapy.UInt64): puyapy.UInt64
{
  new_value: puyapy.UInt64 = x
  one: puyapy.UInt64 = 1u
  new_value += one
  return new_value
}

contract Everything extends (test_cases.everything.my_base::MyMiddleBase, test_cases.everything.my_base::MyBase)
{
  globals {
    ['counter']: puyapy.UInt64
  }
  locals {
    ['name']: puyapy.arc4.String
  }
  
  constructor()
  {
  }
  
  clear_state_program(): bool
  {
    this::_remove_sender()
    return true
  }
  
  abimethod create(): None
  {
    this::_check_ban_list()
    this::remember_creator()
    this.counter: puyapy.UInt64 = 0u
  }
  
  abimethod register(name: puyapy.arc4.String): None
  {
    this::_check_ban_list()
    if (txn<OnCompletion>() == OptIn) {
      (sender_name, sender_name_existed): tuple[puyapy.arc4.String, bool] = STATE_GET_EX(this.name[0u])
      if (!(sender_name_existed)) {
        this.counter += test_cases.everything.my_base::multiplicative_identity()
      }
    }
    this.name[0u]: puyapy.arc4.String = name
  }
  
  abimethod say_hello(): puyapy.arc4.String
  {
    this::_check_ban_list()
    (name, exists): tuple[puyapy.arc4.String, bool] = STATE_GET_EX(this.name[0u])
    if (!(exists)) {
      return arc4_encode('Howdy stranger!', puyapy.arc4.String)
    }
    return arc4_encode('Hello, ', puyapy.arc4.String) + name + arc4_encode('!', puyapy.arc4.String)
  }
  
  abimethod calculate(a: puyapy.arc4.UInt64, b: puyapy.arc4.UInt64): puyapy.arc4.UInt64
  {
    c: puyapy.arc4.UInt64 = test_cases.everything.my_base::MyMiddleBase::calculate(a, b)
    return arc4_encode(arc4_decode(c, puyapy.UInt64) * arc4_decode(b, puyapy.UInt64), puyapy.arc4.UInt64)
  }
  
  abimethod close_out(): None
  {
    this::_remove_sender()
  }
  
  subroutine _check_ban_list(): None
  {
    assert(txn<Sender>() != test_cases.everything.contract::get_banned(), comment="You are banned, goodbye")
  }
  
  subroutine _remove_sender(): None
  {
    this.counter -= test_cases.everything.contract::positive_one()
  }
}

subroutine positive_one(): puyapy.UInt64
{
  return 1u
}