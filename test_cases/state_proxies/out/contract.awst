contract StateProxyContract
{
  globals {
    ['g1']: puyapy.UInt64
    ['g2']: puyapy.UInt64
  }
  locals {
    ['l1']: puyapy.UInt64
    ['l2']: puyapy.UInt64
  }
  
  constructor()
  {
    this.global2: puyapy.UInt64 = 0u
  }
  
  abimethod create(): None
  {
    this.global1: puyapy.UInt64 = 1u
    this.local1[txn<Sender>()]: puyapy.UInt64 = 2u
    this.local2[txn<Sender>()]: puyapy.UInt64 = 3u
  }
}