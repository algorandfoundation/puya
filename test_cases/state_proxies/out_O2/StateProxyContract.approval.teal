#pragma version 10
#pragma typetrack false

// algopy.arc4.ARC4Contract.approval_program() -> uint64:
main:
    intcblock 42 1 0
    bytecblock "l1" "g1" "box_map" 0x6c32
    txn ApplicationID
    bnz main_after_if_else@2
    pushbytes 0x6732
    intc_2 // 0
    app_global_put
    pushbytess "town" "funky" // "town", "funky"
    txn NumAppArgs
    select
    pushint 123 // 123
    app_global_put

main_after_if_else@2:
    pushbytes 0x4c5c61ba // method "create()void"
    txna ApplicationArgs 0
    match main_create_route@4
    txn OnCompletion
    !
    assert // OnCompletion must be NoOp
    txn ApplicationID
    assert
    pushbytess 0x44977246 0xbd8868b9 0xd0e289b0 0x74445702 // method "clear()void", method "order_of_eval_global()void", method "order_of_eval_local()void", method "order_of_eval_box()void"
    txna ApplicationArgs 0
    match main_clear_route@7 main_order_of_eval_global_route@8 main_order_of_eval_local_route@9 main_order_of_eval_box_route@10
    err

main_order_of_eval_box_route@10:
    callsub get_key
    bytec_2 // "box_map"
    swap
    concat
    callsub default_value
    swap
    box_get
    swap
    btoi
    swap
    select
    intc_0 // 42
    ==
    return

main_order_of_eval_local_route@9:
    callsub get_account
    callsub default_value
    swap
    intc_2 // 0
    bytec_0 // "l1"
    app_local_get_ex
    select
    intc_0 // 42
    ==
    return

main_order_of_eval_global_route@8:
    callsub default_value
    intc_2 // 0
    bytec_1 // "g1"
    app_global_get_ex
    select
    intc_0 // 42
    ==
    return

main_clear_route@7:
    bytec_1 // "g1"
    app_global_del
    txn Sender
    bytec_0 // "l1"
    app_local_del
    txn Sender
    bytec_3 // 0x6c32
    app_local_del
    callsub get_key
    bytec_2 // "box_map"
    swap
    concat
    box_del
    pop
    intc_1 // 1
    return

main_create_route@4:
    txn OnCompletion
    intc_1 // OptIn
    ==
    txn ApplicationID
    !
    &&
    assert // OnCompletion must be OptIn && can only call when creating
    bytec_1 // "g1"
    intc_1 // 1
    app_global_put
    txn Sender
    bytec_0 // "l1"
    pushint 2 // 2
    app_local_put
    txn Sender
    bytec_3 // 0x6c32
    pushint 3 // 3
    app_local_put
    intc_1 // 1
    return


// test_cases.state_proxies.contract.get_key() -> bytes:
get_key:
    pushbytes "key"
    log
    pushbytes 0x626f78
    retsub


// test_cases.state_proxies.contract.get_account() -> bytes:
get_account:
    pushbytes "account"
    log
    txn Sender
    retsub


// test_cases.state_proxies.contract.default_value() -> uint64:
default_value:
    pushbytes "default"
    log
    intc_0 // 42
    retsub
