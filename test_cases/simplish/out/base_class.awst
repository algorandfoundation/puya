abstract contract CallCounter
{
  globals {
    ['counter']: uint64
  }
  locals {
    ['name']: bytes
  }
  
  constructor()
  {
    this.counter: uint64 = 0u
  }
  
  subroutine increment_counter(): void
  {
    this.counter += 1u
  }
  
  subroutine set_sender_nickname(nickname: bytes): void
  {
    this.name[0u]: bytes = nickname
  }
}