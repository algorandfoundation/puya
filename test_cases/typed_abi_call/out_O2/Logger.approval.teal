// test_cases.typed_abi_call.logger.Logger.log_bool(value: bytes) -> void:
log_bool:
    proto 1 0
    frame_dig -1
    int 0
    getbit
    bz log_bool_ternary_false@2
    byte "True"
    b log_bool_ternary_merge@3

log_bool_ternary_false@2:
    byte "False"

log_bool_ternary_merge@3:
    log
    retsub


