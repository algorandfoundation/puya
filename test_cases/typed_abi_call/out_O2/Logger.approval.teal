// test_cases.typed_abi_call.logger.Logger.echo(value: bytes) -> bytes:
echo:
    proto 1 1
    frame_dig -1
    extract 2 0
    byte "echo: "
    swap
    concat
    dup
    len
    itob
    extract 6 0
    swap
    concat
    retsub


