#pragma version 10

test_cases.typed_abi_call.typed_c2c.Greeter.approval_program:
    txn NumAppArgs
    bz main_bare_routing@7
    method "test_method_selector_kinds(application)void"
    method "test_arg_conversion(application)void"
    method "test_void(application)void"
    txna ApplicationArgs 0
    match main_test_method_selector_kinds_route@2 main_test_arg_conversion_route@3 main_test_void_route@4
    err // reject transaction

main_test_method_selector_kinds_route@2:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    txna ApplicationArgs 1
    btoi
    txnas Applications
    callsub test_method_selector_kinds
    int 1
    return

main_test_arg_conversion_route@3:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    txna ApplicationArgs 1
    btoi
    txnas Applications
    callsub test_arg_conversion
    int 1
    return

main_test_void_route@4:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    txna ApplicationArgs 1
    btoi
    txnas Applications
    callsub test_void
    int 1
    return

main_bare_routing@7:
    txn OnCompletion
    !
    assert // reject transaction
    txn ApplicationID
    !
    assert // is creating
    int 1
    return


