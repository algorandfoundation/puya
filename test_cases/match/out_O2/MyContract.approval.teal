    b*
    byte 0x
    byte 0x0a
    uncover 2
    match match_biguint_switch_case_0@1 match_biguint_switch_case_1@2
    retsub

match_biguint_switch_case_0@1:
    byte 0x48656c6c6f2062696775696e74
